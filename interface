interface Shape
{
    double area();
}
class Circle implements Shape{
    double radius;
    Circle(double r){
        radius=r;
    }
    public double area(){
        return Math.PI*radius*radius;
    }
    
}
class Rectangle implements Shape
{

    double length,breath;
    Rectangle(double l,double b)
    {
    length=l;
    breath=b;
    }
    public double area()
    {
        return length*breath;
    }

}
class Demo{

    static int count;
    final double PI=3.14159;
    static{
        count=0;
        System.out.println("Static block execute,Initial count="  +count);

    }
    {
    count++;
    System.out.println("Instance Block Execute");
}
Demo(){
    System.out.println("Default Constructor called");
}
final void displayMessage(){
    System.out.println(" This is Final method and cannot Override");
}
void Show(int a){
    System.out.println("Show with int=" +a);
}
void Show(String s){
    System.out.println("Show with String=" +s);
}
}
public class InterfaceDemo {

    public static void main(String[] args)
    {
        Shape s;
        s=new Circle(11);
        System.out.println("Area of Circle="  +s.area());
        s=new Rectangle(9,8);
        System.out.println("Area of Rectangle="  +s.area());
        Demo d1=new Demo();
        d1.displayMessage();
        d1.Show(500);
        d1.Show("Object Oriented Language");
        Demo d2=new Demo();
        d2.displayMessage();
    }
}
